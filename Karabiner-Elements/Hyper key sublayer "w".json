{
  "description": "Hyper Key sublayer \"w\"",
  "manipulators": [
    {
      "description": "Toggle Hyper sublayer w",
      "from": {
        "key_code": "w",
        "modifiers": {
          "mandatory": [
            "right_command",
            "right_control",
            "right_shift",
            "right_option"
          ]
        }
      },
      "to": [
        {
          "set_variable": {
            "name": "hyper_sublayer_w",
            "value": 1
          }
        }
      ],
      "to_after_key_up": [
        {
          "set_variable": {
            "name": "hyper_sublayer_w",
            "value": 0
          }
        }
      ],
      "type": "basic"
    },
    {
